        <div class="chartHeader" style="margin-left: auto; margin-right: auto; width: 700px;">
            <table class="hosts">
 	    <tr>
   	        <th>Source</th>
	        <th></th>
	        <th>Destination</th>
	    </tr>
	    [% size = sources.size %]
	    [% size = size - 1 %]
	    [% FOREACH n IN [0 .. size] %]
	      <tr class="host_table_[% n mod 2 %]">
		<td>
		   <span class="host_table_host" id="source_host_[% n %]">
                       <span class="host_table_looking_up">Retrieving...</span>
                   </span>
                   - [% sources.$n %]
		</td>
		<td>
		   --
		</td>
	      	<td>
		   <span class="host_table_host" id="dest_host_[% n %]">
                       <span class="host_table_looking_up">Retrieving...</span>
                   </span> 
                   - [% dests.$n %]
                </td>
  	      </tr>
              <tr class="host_table_[% n mod 2 %]">
                <td>Capacity: 
                    <span id="source_capacity_[% n %]" style="margin-right: 10px;">Unknown</span>
                    MTU: <span id="source_mtu_[% n %]">Unknown</span>
                </td>
		<td></td>
                <td>Capacity: 
                    <span id="dest_capacity_[% n %]" style="margin-right: 10px;">Unknown</span>
                    MTU: <span id="dest_mtu_[% n %]">Unknown</span>
                </td>
              </tr>
            [% END %]
            </table>
        </div>


    <div id="chart" style="width:890px;height:465px; margin-left: auto; margin-right: auto;">

        <div id="ps-all-tests" class="chart">

